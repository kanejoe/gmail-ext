{
  "name": "Gmail Test Extension",
  "version": "0.1",
  "description": "An extension to test the features of Gmailr.",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "http://mail.google.com/*"],
      "js": [],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/"
  ],
  "manifest_version" : 2,
  "web_accessible_resources" : [ ]
}